{
    "name": "react-ded-boiler",
    "version": "4.0.0",
    "main": "index.js",
    "license": "MIT",
    "description": "React.js Template/Boilerplate to bootstrap a react app with customized configurations. Updated regularly and used in several of my projects as an alternative to Create-React-App.",
    "scripts": {
        "install": "npm install && npm update",
        "lint": "eslint \"src/**/*.{js,jsx}\"",
        "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
        "prettier": "npx prettier --write .",
        "test:webpack": "npx webpack --config webpack.config.js",
        "build": "webpack --mode production",
        "start": "webpack serve --mode development"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bahim22/react-ded-boiler.git"
    },
    "keywords": [
        "React, Webpack, Babel, PostCSS, boilerplate, development"
    ],
    "author": "Hima Balde (Dionysus Era)",
    "bugs": {
        "url": "https://github.com/bahim22/react-ded-boiler/issues"
    },
    "homepage": "https://github.com/bahim22/react-ded-boiler#readme",
    "dependencies": {
        "prop-types": "^15.8.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^6.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-react": "^7.14.5",
        "autoprefixer": "^10.4.1",
        "babel-loader": "^8.2.3",
        "css-loader": "^6.5.1",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.28.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "postcss-loader": "^6.2.1",
        "prettier": "^2.5.1",
        "style-loader": "^3.3.1",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.7.2"
    }
}
